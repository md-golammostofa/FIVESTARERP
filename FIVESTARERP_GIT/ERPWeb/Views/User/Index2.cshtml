                                    </tbody>
                                </table>
                                @*<p><b><i>Today's ReceiveJob</i></b></p>
                                    <h3>
                                        @foreach (var job in (IEnumerable<DashboardDailyReceiveJobOrderViewModel>)ViewBag.DashboardTotalReceiveViewModel)
                                        {
                                            <b><i>@job.Total</i></b>
                                        }
                                    </h3>*@
                            </div>
                            <div class="icon">
                                <i class="fas fa-download"></i>
                            </div>
                            <a href="#" class="small-box-footer"></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-9" style="height:auto">
            <div class="card shadow" style="background-image:url(../../Content/Images/pic15.jpg)">
                <div class="card-header" style="background-image:url(../../Content/Images/pic16.jpg)">
                    <h3 class="card-title text-bold"><i class="fas fa-signal"></i> Last 30 Days Sells Graph</h3>
                    <div class="card-tools">
                        <button type="button" class="btn btn-tool" data-card-widget="collapse">
                            <i class="fas fa-minus"></i>
                        </button>
                        <button type="button" class="btn btn-tool" data-card-widget="remove">
                            <i class="fas fa-times"></i>
                        </button>
                    </div>
                </div>
                <div class="card-body">
                    <div style="width:100%;height:80%;">
