                    </div>

                    <div class="col-sm-3 col-6">
                        <div class="small-box text-white" style="background-color:olive">
                            <div class="inner" style="height:115px">
                                <table>
                                    <tbody>
                                        @foreach (var Bill in (IEnumerable<DashboardDailyReceiveJobOrderViewModel>)ViewBag.DashboardTotalReceiveViewModel)
                                        {
                                            <tr>
                                                <td><b><i>Receive Job : </i></b></td>
