                                <i class="fas fa-recycle"></i>
                            </div>
                            <a href="#" class="small-box-footer"></a>
                        </div>
                    </div>

                    <div class="col-sm-3 col-6">
                        <div class="small-box bg-gradient-orange text-white">
                            <div class="inner" style="height:115px">
                                <table>
                                    <tbody>
                                        @foreach (var Bill in (IEnumerable<DashboardDailyReceiveJobOrderViewModel>)ViewBag.DashboardTotalTransferViewModel)
                                        {
                                            <tr>
                                                <td><b><i>Transfer Job : </i></b></td>
