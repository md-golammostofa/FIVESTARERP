                    <div class="inner" style="height:115px">
                        <p><b><i>Not-Assign</i></b></p>
                        <h3>
                            @foreach (var assign in (IEnumerable<DashboardDailyReceiveJobOrderViewModel>)ViewBag.DashboardNotAssignJobViewModel)
