@model IEnumerable<ERPBO.FrontDesk.ViewModels.JobOrderViewModel>
@{
    var pagerData = (ERPWeb.Infrastructure.PagerData)ViewBag.PagerData;
    int sl = pagerData.Serial;
}
<div class="row">
    <table class="table table-responsive-lg table-bordered table-sm tblJobOrderAllBranch" id="tblJobOrderAllBranch">
        <thead class="btn-dark">
            <tr>
                <th style="vertical-align:middle">#SL</th>
                <th style="vertical-align:middle">Job Code</th>
                <th style="vertical-align:middle">Customer Name</th>
                <th style="vertical-align:middle">Model</th>
                <th style="vertical-align:middle">IMEI</th>
                <th style="vertical-align:middle">StateStatus</th>
                <th style="vertical-align:middle">Job Type</th>
                <th style="vertical-align:middle">Type</th>
                <th style="vertical-align:middle">TS Status</th>
                <th style="vertical-align:middle">Customer Type</th>
                <th style="vertical-align:middle">Date</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                sl = sl + 1;
                <tr>
                    <td>@sl</td>
                    <td>@item.JobOrderCode</td>
                    <td>@item.CustomerName</td>
                    <td>@item.ModelName</td>
                    <td>@item.IMEI</td>
                    <td>@item.StateStatus</td>
                    <td>@item.JobOrderType</td>
                    <td>@item.Type</td>
                    <td>@item.TsRepairStatus</td>
                    <td>@item.CustomerType</td>
                    <td>@item.EntryDate.Value.ToString("dd-MMM-yyyy")</td>
                </tr>
            }
        </tbody>
    </table>
    <div class="col-md-12 joborder">
        @Html.Partial("_Pagination", pagerData)
    </div>
</div>
