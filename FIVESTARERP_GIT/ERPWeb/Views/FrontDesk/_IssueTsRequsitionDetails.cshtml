@model IEnumerable<ERPBO.FrontDesk.ViewModels.RequsitionDetailForJobOrderViewModel>
@{ 
    Layout = null;
    int sl = 0;
}
<table class="table table-bordered table-sm table-striped table-responsive-lg tblDataDetail text-sm" id="tblDataDetail">
    <thead class="btn-dark">
        <tr class="text-center text-sm">
            <th>#SL</th>
            <th class="hide"></th>
            <th class="hide"></th>
            <th>Model</th>
            <th class="hide"></th>
            <th>Parts Name</th>
            <th>Parts Code</th>
            <th>Request Qty</th>
            <th>Issue Qty</th>
            <th>Available Qty</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            sl = sl + 1;
        <tr class="text-sm text-bold">
            <td class="text-center text-bold" style="width:5%">@sl</td>
            <td class="hide">@item.RequsitionDetailForJobOrderId</td>
            <td class="hide">@item.DescriptionId</td>
            <td>@item.ModelName</td>
            <td class="hide">@item.PartsId</td>
            <td>@item.PartsName</td>
            <td>@item.MobilePartCode</td>
            <td class="text-center" style="width:10%">@item.Quantity</td>
            <td style="width:10%">
                <input type="number" class="issueQty form-control form-control-sm" value="0" />
            </td>
            <td class="text-center" style="width:10%;color:red;">
                @item.AvailableQty
            </td>
        </tr>
        }
    </tbody>
</table>