            hdfInfoId.val('0');
            hdfRowIndex.val("");
            $("#modalIQCReqDetails").modal("show");

        }

        $(document).on("click", ".data-details", function (e) {

            e.preventDefault();
            var id = $(this).attr("data-details");

            var data = {
                reqInfoId: TryParseInt(id, 0)
            };
            var idx = $(this).parent().parents('tr').index();

            console.log(data);
            //return;
            $.when(getReqWithData('html', 'GET', '/Inventory/GetIQCItemReqDetailList', data)).then(function (res, status) {
                if (status == "success") {
                    OpenModal();
                    hdfInfoId.val(data.reqInfoId);
                    hdfRowIndex.val(idx);
                    $("#modelContainer1").empty();
                    $("#modelContainer1").append(res);
                    $(".btnReacive-Return").addClass('hide');
                }
            }).fail(function (error) {
                console.log(error);
            })
        })

        ddlWarehouseForIQCReturn.change(function () {

            LoadDataTableForIQCReturn();
        })

        ddlModelNameForIQCReturn.change(function () {
            LoadDataTableForIQCReturn();
        })

        ddlStateStatusForIQCReturn.change(function () { LoadDataTableForIQCReturn(); })

        function LoadDataTableForIQCReturn() {
            var data = { WarehouseId: TryParseInt(ddlWarehouseForIQCReturn.val(), 0), modelId: TryParseInt(ddlModelNameForIQCReturn.val(), 0), stateStatus: ddlStateStatusForIQCReturn.val(), page: pageNoForIQCReturn };
            $.when(getReqWithData('html', 'GET', '/Inventory/GetIQCReturnDataInfoList', data)).then(function (res, status) {
                console.log(status);
                if (status === "success") {
                    $("#dataContainer3").fadeOut('500', function () {
                        $("#dataContainer3").empty();
                        $("#dataContainer3").append(res).fadeIn('500');
                    });
                }
            }).fail(function (error) {
                console.log(error);
            });
            pageNoForIQCReturn = 1;
        }

        // Pagination
        $(document).on('click', 'a.page-link', function (e) {
            e.preventDefault();
            if (!$(this).hasClass('current-page')) {
                //alert('Page Clicked');
                if ($(this).parent().parents('div').hasClass('IQCItemReturnInfo')) {
                    //alert('Page Found');
                    pageNoForIQCReturn = $(this).attr('data-page-no');
                    LoadDataTableForIQCReturn();
                }

            }
        })

        $("#btnResetForIQCReturn").click(function (e) {
            e.preventDefault();
            ddlItem.val('');
            ddlItem.trigger("change");
            ddlWarehouseForIQCReturn.val("");
            ddlWarehouseForIQCReturn.trigger("change");
            ddlModelNameForIQCReturn.val('');
            ddlModelNameForIQCReturn.trigger("change");
            ddlStateStatusForIQCReturn.val('');
            ddlStateStatusForIQCReturn.trigger("change");
            $("#tblIQCItemReqInfoData tbody").empty();
        })
    </script>
}