                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span></button>
                </div>
                <div class="modal-body">
                    <form id="frmRSM">
                        <input type="hidden" name="Id" id="hdfRSMId" />
                        <input type="hidden" name="Id" id="hdfRSMUserId" />
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label for="txtRSMFullName" class="control-label font-weight-bold">
                                    Full Name
                                    <div class="checkbox d-inline icheck-emerland">
                                        <input type="checkbox" name="chkRSMStatestatus" id="chkRSMStatestatus" />
                                        <label for="chkRSMStatestatus" class="text-success">
                                            Active
                                        </label>
                                    </div>
                                </label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-user"></i>
                                        </span>
                                    </div>
                                    <input type="text" name="txtRSMFullName" value="" id="txtRSMFullName" class="form-control form-control-sm" />
                                </div>
                                <span class="error hide required-rsm-fillName font-weight-bold">Input FullName !</span>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="txtRSMAddress" class="control-label font-weight-bold">Address</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-home"></i>
                                        </span>
                                    </div>
                                    <input type="text" name="txtRSMAddress" value="" id="txtRSMAddress" class="form-control form-control-sm" />
                                </div>
                                <span class="error hide required-rsm-address font-weight-bold">Input Adderess!</span>
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label for="txtRSMMobile" class="control-label font-weight-bold">Moblie</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-mobile"></i>
                                        </span>
                                    </div>
                                    <input type="text" name="txtRSMMobile" value="" id="txtRSMMobile" class="form-control form-control-sm" />
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="txtRSMEmail" class="control-label font-weight-bold">Email</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-envelope"></i>
                                        </span>
                                    </div>
                                    <input type="text" name="txtRSMEmail" value="" id="txtRSMEmail" class="form-control form-control-sm" />
                                </div>
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label for="ddlRSMDistrict" class="control-label font-weight-bold">District</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-road"></i>
                                        </span>
                                    </div>
                                    @Html.DropDownList("ddlRSMDistrict", (IEnumerable<SelectListItem>)ViewBag.ddlDistrictWithDivision, "---Select District---", new { @class = "form-control form-control-sm" })
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="txtRMSRemarks" class="control-label font-weight-bold">Remarks</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-comment"></i>
                                        </span>
                                    </div>
                                    <input type="text" name="txtRMSRemarks" value="" id="txtRMSRemarks" class="form-control form-control-sm" />
                                </div>
                            </div>
                        </div>
