    <div class="modal fade" id="modalDealer" role="dialog" tabindex="-1" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header alert-secondary">
                    <h4 id="modalHeading" class="modal-title"><span id="spanModalHeadDealer">Add New Dealer</span></h4>
                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span></button>
                </div>
                <div class="modal-body">
                    <form id="frmCategory">
                        <input type="hidden" name="Id" id="hdfDealerId" />
                        <input type="hidden" name="Id" id="hdfDealerUserId" />
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label for="txtDealerName" class="control-label font-weight-bold">
                                    Dealer<span class="text-danger">*</span>
                                    <div class="checkbox d-inline icheck-emerland">
                                        <input type="checkbox" name="chkStatestatus" id="chkStatestatus" />
                                        <label for="chkStatestatus" class="text-success">
                                            Active
                                        </label>
                                    </div>
                                </label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-user"></i>
                                        </span>
                                    </div>
                                    <input type="text" name="txtDealerName" value="" id="txtDealerName" class="form-control form-control-sm" />
                                </div>
                                <span class="error hide required-dealerName font-weight-bold">Input Dealer Name!</span>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="txtAddress" class="control-label font-weight-bold">Address<span class="text-danger">*</span></label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-home"></i>
                                        </span>
                                    </div>
                                    <input type="text" name="txtAddress" value="" id="txtAddress" class="form-control form-control-sm" />
                                </div>
                                <span class="error hide required-address font-weight-bold">Input Adderess!</span>
                            </div>
                        </div>

                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label for="txtTelephone" class="control-label font-weight-bold">Telephone</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-phone"></i>
                                        </span>
                                    </div>
                                    <input type="text" name="txtTelephone" value="" id="txtTelephone" class="form-control form-control-sm" />
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="txtMobile" class="control-label font-weight-bold">Moblie<span class="text-danger">*</span></label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-mobile"></i>
                                        </span>
                                    </div>
                                    <input type="text" name="txtMobile" value="" id="txtMobile" class="form-control form-control-sm" />
                                </div>
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label for="txtEmail" class="control-label font-weight-bold">Email</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-envelope"></i>
                                        </span>
                                    </div>
                                    <input type="text" name="txtEmail" value="" id="txtEmail" class="form-control form-control-sm" />
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="txtContactPersonName" class="control-label font-weight-bold">Contact_Person</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-user-shield"></i>
                                        </span>
                                    </div>
                                    <input type="text" name="txtContactPersonName" value="" id="txtContactPersonName" class="form-control form-control-sm" />
                                </div>
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label for="txtContactPersonMobile" class="control-label font-weight-bold">Contact_Mobile</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-mobile"></i>
                                        </span>
                                    </div>
                                    <input type="text" name="txtContactPersonMobile" value="" id="txtContactPersonMobile" class="form-control form-control-sm" />
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="txtRemarks" class="control-label font-weight-bold">Remarks</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-comment"></i>
                                        </span>
                                    </div>
                                    <input type="text" name="txtRemarks" value="" id="txtRemarks" class="form-control form-control-sm" />
                                </div>
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label for="ddlDealerDistrictWithDivision" class="control-label font-weight-bold">District<span class="text-danger">*</span></label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-road"></i>
                                        </span>
                                    </div>
                                    @Html.DropDownList("ddlDealerDistrictWithDivision", (IEnumerable<SelectListItem>)ViewBag.ddlDistrictWithDivision, "---Select District---", new { @class = "form-control form-control-sm select2", @style = "width:80%" })
                                </div>
                                <span class="error hide required-dealer-district font-weight-bold">District is required!</span>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="ddlDealerZone" class="control-label font-weight-bold">Zone<span class="text-danger">*</span></label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-road"></i>
                                        </span>
                                    </div>
                                    <select id="ddlDealerZone" class="form-control form-control-sm" style="width:80%">
                                        <option value="">---Select Zone---</option>
                                    </select>
                                </div>
                                <span class="error hide required-dealer-zone font-weight-bold">Zone is required!</span>
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label for="ddlDealerRepresentative" class="control-label font-weight-bold">Representative<span class="text-danger">*</span></label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-user-shield"></i>
                                        </span>
                                    </div>
                                    @Html.DropDownList("ddlDealerRepresentative", (IEnumerable<SelectListItem>)ViewBag.ddlDealerRepresentative, "---Select Representative---", new { @class = "form-control form-control-sm" })
                                </div>
                                <span class="error hide required-dealer-rep font-weight-bold">Representative is required!</span>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="chkIsDealerAllowToLogin" class="control-label font-weight-bold" style="visibility:hidden">Allow to login</label>
                                <div class="form-control form-control-sm">
                                    <input type="checkbox" name="chkIsDealerAllowToLogin" id="chkIsDealerAllowToLogin" /> Allow to login?
                                </div>
                            </div>
                        </div>
                        <div class="form-row hide" id="dealer-user">
                            <div class="form-group col-md-6">
                                <label for="txtDealerUserName" class="control-label font-weight-bold">User Name<span class="text-danger">*</span></label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-user-cog"></i>
                                        </span>
                                    </div>
                                    <input type="text" name="txtDealerUserName" value="" id="txtDealerUserName" class="form-control form-control-sm" />
                                </div>
                                <span class="error hide required-dealer-userName font-weight-bold">Input User Name!</span>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="txtDealerUserPassword" class="control-label font-weight-bold">Password<span class="text-danger">*</span></label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-key"></i>
                                        </span>
                                    </div>
                                    <input type="text" name="txtDealerUserPassword" value="" id="txtDealerUserPassword" class="form-control form-control-sm" />
                                </div>
                                <span class="error hide required-dealer-password font-weight-bold">Input Password!</span>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="modal-footer btn-light">
                    <button type="button" class="btn btn-danger btn-flat float-right" data-dismiss="modal" data-target="#"><i class="fas fa-times"></i> Cancel </button>
                    <button type="submit" class="btn btn-success btn-flat float-right" id="btnSubmitDealer"><i class="far fa-save"></i> <span id="spanSaveTextDealer"> Save</span> </button>
                </div>
            </div>
        </div>
    </div>
