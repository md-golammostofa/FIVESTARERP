                var data = JSON.stringify({ ModelId: TryParseInt(hdfModelId.val(), 0), ModelName: txtModelName.val(), BrandId: TryParseInt(ddlBrandName.val(),0), Remarks: txtModelRemarks.val() });
                console.log(data);
                $.when(postReqWithToken(dataType.applicationJson, dataType.json, 'POST', '/Configuration/SaveModel', data, getToken())).then(function (res, status) {
                    console.log(status);
                    console.log(res);
                    if (res == true) {
                                $('.toastrDefaultSuccess').trigger('click');
                                setTimeout(function () {
                                    redirectPage('@Url.Action("AccessoriesList", new { tab = "model" })');
                                }, 500);
                            }
                            else {
                                $('.toastrDefaultError').trigger('click');
                    }
                    enable("#btnSubmitModel");
                }).fail(function (err) {
                    console.log(err);
                    enable("#btnSubmitModel");
                });
            }
        })

        // Pagination
        $(document).on('click', 'a.page-link', function (e) {
            e.preventDefault();
            if (!$(this).hasClass('current-page')) {
                if ($(this).parent().parents('div').hasClass('accessories')) {
                    pageNo = $(this).attr('data-page-no');
                    LoadAccessoriesDataTable();
                }
                else if ($(this).parent().parents('div').hasClass('symptom')) {
                    pageNo = $(this).attr('data-page-no');
                    LoadSymptomDataTable();
                }
                else if ($(this).parent().parents('div').hasClass('parts')) {
                    pageNo = $(this).attr('data-page-no');
                    LoadMobilePartsDataTable();
                }
                else if ($(this).parent().parents('div').hasClass('fault')) {
                    pageNo = $(this).attr('data-page-no');
                    LoadFaultDataTable();
                }
                else if ($(this).parent().parents('div').hasClass('services')) {
                    pageNo = $(this).attr('data-page-no');
                    LoadServicesDataTable();
                }
                else if ($(this).parent().parents('div').hasClass('repair')) {
                    pageNo = $(this).attr('data-page-no');
                    LoadRepairDataTable();
                }
                else if ($(this).parent().parents('div').hasClass('dealer')) {
                    pageNo = $(this).attr('data-page-no');
                    LoadDealerDataTable();
                }
                else if ($(this).parent().parents('div').hasClass('color')) {
                    pageNo = $(this).attr('data-page-no');
                    LoadColorDataTable();
                }
                else if ($(this).parent().parents('div').hasClass('brand')) {
                    pageNo = $(this).attr('data-page-no');
                    LoadBrandDataTable();
                }
                else if ($(this).parent().parents('div').hasClass('model')) {
                    pageNo = $(this).attr('data-page-no');
                    LoadModelDataTable();
                }
            }
        })

        function redirectPage(page) {
            window.location.replace(page);
        }
    </script>
