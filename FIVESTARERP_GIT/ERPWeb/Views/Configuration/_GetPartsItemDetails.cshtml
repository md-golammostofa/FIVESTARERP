@model IEnumerable<ERPBO.Configuration.ViewModels.PartsTransferHToCDetailsViewModel>
@{
    int sl = 0;
    Layout = null;
    string badge = string.Empty;

}
<div class="row">
    <div class="col-md-12">
        <table class="table table-bordered table-sm table-striped" id="tblStockTransferInfo">
            <thead class="btn-dark">
                <tr class="text-center">
                    <th>#SL</th>
                    <th class="hide">info</th>
                    <th class="hide">details</th>
                    <th class="hide">ModelId</th>
                    <th>Model</th>
                    <th class="hide">PartsId</th>
                    <th>Parts Name</th>
                    <th>Cost Price</th>
                    <th>Sell Price</th>
                    <th>Quantity</th>

                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    sl = sl + 1;
                    <tr>
                        <td class="text-center" width="10%">@sl</td>
                        <td class="hide">@item.InfoId</td>
                        <td class="hide">@item.DetailsId</td>
                        <td class="hide">@item.ModelId</td>
                        <td>@item.ModelName</td>
                        <td class="hide">@item.PartsId</td>
                        <td>@item.PartsName</td>
                        <td class="text-right">@item.CostPrice.ToString("0.00")</td>
                        <td class="text-right">@item.SellPrice.ToString("0.00")</td>
                        <td class="text-center">@item.Quantity</td>

                    </tr>
                }
                @if (Model.Count() == 0)
                {
                    <tr class="text-center empty-row">
                        <td colspan="5">
                            No Rows Found
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>



