@model IEnumerable<ERPBO.Configuration.ViewModels.DustStockInfoViewModel>
@{ 
    int sl = 0;
}
<div class="row">
    <table class="table table-bordered table-sm table-striped tblFaultyStockScrap" id="tblFaultyStockScrap">
        <thead class="btn-dark">
            <tr class="text-center">
                <th>#SL</th>
                <th class="hide"></th>
                <th class="hide"></th>
                <th>Model</th>
                <th>Parts Name</th>
                <th>Parts Code</th>
                <th>StockIn</th>
                <th>StockOut</th>
                <th>StockQty</th>
            </tr>
        </thead>
        <tbody></tbody>
        <tbody>
            @foreach (var info in Model)
            {
                sl = sl + 1;
                <tr data-item-row="notSelected">
                    <td class="text-center text-bold">@sl</td>
                    <td class="hide">@info.ModelId</td>
                    <td class="hide">@info.PartsId</td>
                    <td class="text-center">@info.ModelName</td>
                    <td class="text-center">@info.PartsName</td>
                    <td class="text-center">@info.PartsCode</td>
                    <td class="text-center">
                        @info.StockInQty
                    </td>
                    <td class="text-center">@info.StockOutQty</td>
                    <td class="text-center">@info.StockQty</td>
                </tr>
            }
            @if (Model.Count() == 0)
            {
                <tr class="text-center empty-row">
                    <td colspan="13">
                        No data available in table
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>