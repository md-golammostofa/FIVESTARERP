@model IEnumerable<ERPBO.Configuration.ViewModels.FaultyStockTransferDetailsViewModel>
@{
    int sl = 0;
}
<table class="table table-bordered table-sm table-striped table-responsive-lg" id="tblTransferStockDetails">
    <thead class="btn-dark">
        <tr class="text-center">
            <th style="vertical-align:middle">#SL</th>
            <th class="hide"></th>
            <th>Model</th>
            <th>Parts Name</th>
            <th>Parts Code</th>
            <th>Qty.</th>
            <th>Receive Qty</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var details in Model)
        {
            sl = sl + 1;
            <tr>
                <td class="text-center text-bold">@sl</td>
                <td class="hide">@details.FSTInfoId</td>
                <td>@details.ModelName</td>
                <td>@details.PartsName</td>
                <td class="text-center">@details.PartsCode</td>
                <td class="text-center">@details.Quantity</td>
                <td class="text-center">@details.ReceiveQty</td>
            </tr>
        }
        @if (Model.Count() == 0)
        {
            <tr class="text-center empty-row">
                <td colspan="14">
                    No data found in table
                </td>
            </tr>
        }
    </tbody>
</table>
