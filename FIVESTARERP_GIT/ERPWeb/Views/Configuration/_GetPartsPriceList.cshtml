@model IEnumerable<ERPBO.Configuration.ViewModels.MobilePartStockInfoViewModel>
@{
    int sl = 0;
    Layout = null;
}
<div class="row">
    <div class="col-md-12">
        <table class="table table-bordered table-sm table-striped" id="tblPartsPriceList">
            <thead class="btn-dark">
                <tr class="text-center">
                    <th>#SL</th>
                    <th class="hide"></th>
                    <th>Model</th>
                    <th class="hide"></th>
                    <th>Parts Name</th>
                    <th>Parts Code</th>
                    <th>Cost Price</th>
                    <th>Sell Price</th>
                </tr>
            </thead>
            <tbody></tbody>
            <tbody>
                @foreach (var info in Model)
                {
                    sl = sl + 1;
                <tr>
                    <td class="text-center text-bold">@sl</td>
                    <td class="hide">@info.DescriptionId</td>
                    <td>@info.ModelName</td>
                    <td class="hide">@info.MobilePartId</td>
                    <td>@info.MobilePartName</td>
                    <td class="text-center">@info.PartsCode</td>
                    <td class="text-right">@info.CostPrice.ToString("0.00")</td>
                    <td class="text-right">@info.SellPrice.ToString("0.00")</td>
                </tr>
                }
                @if (Model.Count() == 0)
                {
                    <tr class="text-center empty-row">
                        <td colspan="6">
                            No data available in table
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
    @*<div class="col-md-12">
            @Html.Partial("_Pagination", pagerData)
        </div>*@
</div>


