@model IEnumerable<ERPBO.Configuration.ViewModels.FaultyStockTransferDetailsViewModel>
@{ 
    int sl = 0;
}
<table class="table table-bordered table-sm table-striped table-responsive-lg tblTransferDataDetail text-sm" id="tblTransferDataDetail">
    <thead class="btn-dark">
        <tr class="text-center text-sm">
            <th>#SL</th>
            <th class="hide"></th>
            <th class="hide"></th>
            <th class="hide"></th>
            <th>Model</th>
            <th class="hide"></th>
            <th>Parts Name</th>
            <th>Parts Code</th>
            <th>Transfer Qty</th>
            <th>Receive Qty</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            sl = sl + 1;
        <tr class="text-sm text-bold">
            <td class="text-center text-bold" style="width:5%">@sl</td>
            <td class="hide">@item.FSTInfoId</td>
            <td class="hide">@item.FSTDetailsId</td>
            <td class="hide">@item.ModelId</td>
            <td>@item.ModelName</td>
            <td class="hide">@item.PartsId</td>
            <td>@item.PartsName</td>
            <td>@item.PartsCode</td>
            <td class="text-center" style="width:10%">@item.Quantity</td>
            <td style="width:10%">
                <input type="number" class="receiveQty form-control form-control-sm" value="0" />
            </td>
        </tr>
        }
    </tbody>
</table>