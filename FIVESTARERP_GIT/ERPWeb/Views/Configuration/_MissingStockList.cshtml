@model IEnumerable<ERPBO.Configuration.ViewModels.MissingStockViewModel>
@{
    var pagerData = (ERPWeb.Infrastructure.PagerData)ViewBag.PagerData;
    int sl = pagerData.Serial;
}
<div class="row">
    <table class="table table-bordered table-sm table-striped" id="tblMissingStock">
        <thead class="btn-dark">
            <tr class="text-center">
                <th>#SL</th>
                <th class="hide"></th>
                <th class="hide"></th>
                <th class="hide"></th>
                <th class="hide"></th>
                <th>Model</th>
                <th>Color</th>
                <th>Mobile Parts</th>
                <th>IMEI</th>
                <th>Type</th>
                <th>Quantity</th>
            </tr>
        </thead>
        <tbody></tbody>
        <tbody>
            @foreach (var info in Model)
            {
                sl = sl + 1;
            <tr>
                <td class="text-center text-bold">@sl</td>
                <td class="hide">@info.MissingStockId</td>
                <td class="hide">@info.DescriptionId</td>
                <td class="hide">@info.ColorId</td>
                <td class="hide">@info.PartsId</td>
                <td class="text-center" data-val="@info.DescriptionId">@info.ModelName</td>
                <td class="text-center" data-val="@info.ColorId">@info.ColorName</td>
                <td class="text-center" data-val="@info.PartsId">@info.PartsName</td>
                <td class="text-center">@info.IMEI</td>
                <td class="text-center">@info.StockType</td>
                <td class="text-center">@info.Quantity</td>
                @*<td class="text-center">
                    <a href="#" class="btn btn-warning btn-sm data-edit-item" data-edit-item="@info.MissingStockId" style="line-height:1"><i class="fa fa-pen"></i>Edit</a>
                </td>*@
            </tr>
            }
            @if (Model.Count() == 0)
            {
                <tr class="text-center empty-row">
                    <td colspan="13">
                        No data available in table
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
<div class="missingStock">
    @Html.Partial("_Pagination", pagerData)
</div>


