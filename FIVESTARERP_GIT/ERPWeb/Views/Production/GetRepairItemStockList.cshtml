@{
    ViewBag.Title = "Repair Item Stock Info";
    var privilege = (ERPBO.Common.UserPrivilege)ViewBag.UserPrivilege;
}
<div class="row">
    <div class="col-md-12">
        <div class="card card-primary card-outline card-tabs">
            <div class="card-header p-0 pt-1 border-bottom-0">
                <ul class="nav nav-tabs" id="custom-tabs-two-tab" role="tablist">
                    <li class="nav-item">
                        <a class="nav-link active" id="custom-tabs-two-stockInfo-tab" data-toggle="pill" href="#custom-tabs-two-stockInfo" role="tab" aria-controls="custom-tabs-two-stockInfo" aria-selected="true">Repair Item Stock</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" id="custom-tabs-two-receiveStock-tab" data-toggle="pill" href="#custom-tabs-two-receiveStock" role="tab" aria-controls="custom-tabs-two-receiveStock" aria-selected="true">Receive QC Item</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" id="custom-tabs-two-transferList-tab" data-toggle="pill" href="#custom-tabs-two-transferList" role="tab" aria-controls="custom-tabs-two-transferList" aria-selected="true">Transfer To QC</a>
                    </li>
                </ul>
            </div>
            <div class="card-body">
                <div class="tab-content" id="custom-tabs-two-tabContent">
                    <div class="tab-pane fade show active" id="custom-tabs-two-stockInfo" role="tabpanel" aria-labelledby="custom-tabs-two-stockInfo-tab">
                        <div class="row" style="margin-top:-15px">
                            <div class="col-md-12">
                                <div class="card card-gray-dark">
                                    <div class="card-header">
                                        <div class="row">
                                            <div class="col-md-3">
                                                <a href="#" class="float-left btn btn-sm btn-outline-primary" title="Details View">
                                                    <i class="fas fa-eye"></i> Go To Details View
                                                </a>
                                            </div>
                                            <div class="col-md-6">
                                                <h5 class="text-center text-bold">
                                                    Repair Item Stock
                                                </h5>
                                            </div>
                                            <div class="col-md-3">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row text-sm">
                            <div class="col-md-12" style="margin-top:-10px">
                                <div class="card card-navy">
                                    @Html.AntiForgeryToken()
                                    <div class="card-header">
                                        <h6 class="card-title">Search By</h6>
                                    </div>
                                    <div class="card-body">
                                        <div class="row" style="margin-top:-15px">
                                            <div class="col-md-3">
                                                <label for="ddlFloorItemStock" class="control-label font-weight-bold">Floor</label>
                                                @Html.DropDownList("ddlFloorItemStock", (IEnumerable<SelectListItem>)ViewBag.ddlFloor, "--Select Floor--", new { @class = "form-control form-control-sm ctrl-change" })
                                            </div>
                                            <div class="col-md-3">
                                                <label for="ddlQCLineItemStock" class="control-label font-weight-bold">QC Line</label>
                                                <select id="ddlQCLineItemStock" class="form-control form-control-sm">
                                                    <option value="0">--Select QC Line--</option>
                                                </select>
                                            </div>
                                            <div class="col-md-3">
                                                <label for="ddlRepairLineItemStock" class="control-label font-weight-bold">Repair Line</label>
                                                <select id="ddlRepairLineItemStock" class="form-control form-control-sm">
                                                    <option value="0">--Select Repair Line--</option>
                                                </select>
                                            </div>
                                            <div class="col-md-3">
                                                <label for="ddlModelNameItemStock" class="control-label font-weight-bold">Model</label>
                                                @Html.DropDownList("ddlModelNameItemStock", (IEnumerable<SelectListItem>)ViewBag.ddlModelName, "--Select Model--", new { @class = "form-control form-control-sm ctrl-change select2 select2-danger" })
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-4">
                                                <label for="ddlItems" class="control-label font-weight-bold">Item Name</label>
