                                LoadDataTableRepairSectionSemiFinishGoodStockInfo();
                                $("#modalHalfDoneTransferToWarehouseDetails").modal("toggle"); 
                            }
                            enable('.btnHalfDoneTransferReceive');
                        }).fail(function (error) {
