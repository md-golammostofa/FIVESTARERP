                format: "dd MM yyyy",
                orientation: "bottom auto",
                todayHighlight: true
            }).on('change', function () {
                $('.datepicker').hide();
            });
            $('#dptToDateForHalfDoneStockTransferToWarehouseSearch').datepicker({
                format: "dd MM yyyy",
                orientation: "bottom auto",
                todayHighlight: true
            }).on('change', function () {
                $('.datepicker').hide();
            });

            initialLoad();
        })

        function initialLoad() {
            loadMiniStockTransferToWarehouseListDataTable();
            LoadDataTable();
            loadMiniStockRequisitionToSemiFinishWarehouseListDataTable();
            loadHalfDoneStockTranferListDataTable();
        }

        ddlProductionFloor.change(function () {
            LoadDataTable();
        })

        ddlModelName.change(function () {
            LoadDataTable();
        })

        txtLessOrEq.keyup(function () {
            LoadDataTable();
        })

        ddlItem.change(function () {
            LoadDataTable();
        })

        function LoadDataTable() {
            var item = ddlItem.val() != "" ? ddlItem.val().split("#") : ["0", "0", "0"];
            var data = { flag: "search", floorId: ddlProductionFloor.val(), modelId: ddlModelName.val(), warehouseId: TryParseInt(item[2], 0), itemTypeId: TryParseInt(item[1], 0), itemId: TryParseInt(item[0], 0), lessOrEq: txtLessOrEq.val() };

            $.when(getReqWithData('html', 'GET', '/Production/GetProductionAssembleItemStockInfo', data)).then(function (res, status) {
                console.log(status);
                if (status === "success") {
                    $("#dataContainer1").fadeOut('500', function () {
                        $("#dataContainer1").empty();
                        $("#dataContainer1").append(res).fadeIn('500');
                    });
                }
            }).fail(function (error) {
                console.log(error);
            });
            pageNo = 1;
        }

        // QC Pass //

        ddlLineNumber2.change(function () {
            LoadDataTable2();
            //clearDropdown("ddlQCLine2");
            //if (ddlLineNumber2.val() != "") {
            //    LoadDropDown('/Common/GetQCByLine', 'POST', ddlQCLine2, JSON.stringify({ lineId: ddlLineNumber2.val() }));
            //}
        })

        ddlModelName2.change(function () {
            LoadDataTable2();
        })

        //ddlQCLine2.change(function () {
        //    LoadDataTable2();
        //})

        ddlWarehouse2.change(function () {
            LoadDataTable2();
        })

        function LoadDataTable2() {

            var data = { flag: "search", floorId: TryParseInt(ddlLineNumber2.val(), 0), qcId: TryParseInt(ddlQCLine2.val(), 0), modelId: TryParseInt(ddlModelName2.val(), 0), warehouseId: TryParseInt(ddlWarehouse2.val(), 0) };

            $.when(getReqWithData('html', 'GET', '/Production/GetQCPassList', data)).then(function (res, status) {
                console.log(status);
                if (status === "success") {
                    $("#dataContainer2").fadeOut('500', function () {
                        $("#dataContainer2").empty();
                        $("#dataContainer2").append(res).fadeIn('500');
                    });
                }
            }).fail(function (error) {
                console.log(error);
            });
        }


        $(document).on('click', 'a.data-qcPass-item-detail', function (e) {
            e.preventDefault();
            var id = $(this).attr("data-qcPass-item-detail");
            if (id > 0) {
                var data = { flag: "detail", floorId: 0, assemblyId: 0, modelId: 0, qcId: 0, warehouseId: 0, itemTypeId: 0, itemId: 0, status: '', qcPassCode: '', qcPassId: TryParseInt(id, 0), fromDate: '', toDate: '' };
                $.when(getReqWithData('html', 'GET', '/Production/GetQCPassList', data)).then(function (res, status) {
                    console.log(status);
                    if (status === "success") {
                        $("#modalContainer1").empty();
                        $("#modalContainer1").append(res);
                        $("#modalTransferDetails").modal("toggle");
                    }
                }).fail(function (error) {
                    console.log(error);
                });
            }
        })

        $(document).on('click', 'a.data-item-receive', function (e) {
            e.preventDefault();
            hdfQCPassId.val('0');
            var id = $(this).attr('data-item-val');
            $("#modalQCPass").modal("show");
            hdfQCPassId.val(id);

            var row = $(this).parent().parents('tr');

            var tblQcPass = $('#tblQcPass tbody');
            var qcPassRows = tblQcPass.children('tr');


            var qcPassRow1 = qcPassRows.eq('0');
            console.log(qcPassRow1);
            var row1Cell2 = qcPassRow1.children('td').eq(1).html(row.children('td').eq(1).html());
            //var row1Cell4 = qcPassRow1.children('td').eq(3).html();

            var qcPassRow2 = qcPassRows.eq('1');
            var row2Cell2 = qcPassRow2.children('td').eq(1).html(row.children('td').eq(2).html());
            var row2Cell2 = qcPassRow2.children('td').eq(3).html(row.children('td').eq(4).html());

            var rowStatus = row.children('td').eq(5).attr("data-val");
            if (rowStatus == "Received By Production") {
                $("#btnSubmitQCPass").addClass('hide');
            }
            else {
                $("#btnSubmitQCPass").removeClass('hide');
            }
        })



        $("#btnSubmitQCPass").click(function (e) {
            e.preventDefault();
            if (TryParseInt(hdfQCPassId.val(), 0) > 0) {
                bootbox.confirm("Are you sure you want to sure Receive this QC Pass Items", function (result) {
                    if (result) {
                        disable("#btnSubmitQCPass");
                        var data = JSON.stringify({ qcPassId: TryParseInt(hdfQCPassId.val(), 0), status: "Received By Production" });

                        $.when(postReqWithToken(dataType.applicationJson, dataType.json, type.post, '/Production/SaveQCPassItemReceive', data, getToken())).then(function (res, status) {
                            if (res === true && status === "success") {
                                $('.toastrDefaultSuccess').trigger('click');
                                $("#modalQCPass").modal("toggle");
                                LoadDataTable2();
