                            <div class="col-md-12" style="margin-top:-10px">
                                <div class="card">
                                    <div class="card-body">
                                        <div class="col-md-12" id="dataContainer2">
                                            @{Html.RenderAction("GetPackagingFinishGoodsProcess", new { @flag = "transferinfo" });}
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
