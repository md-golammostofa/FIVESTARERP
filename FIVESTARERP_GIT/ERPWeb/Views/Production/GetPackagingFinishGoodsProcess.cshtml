        function resetFinishGoodsPackagingUI() {
            $("#tblIMEIItem tbody").empty();
            txtIMEIScaning.val('');
            //txtNetWeight.val('');
            //txtGrossWeight.val('');
            txtIMEIScaning.focus();
            enable($("#ddlModel"));
            enable($("#ddlItemName"));
            enable($("#ddlPackagingLinePacking"));
        }

        function findDuplication(imei) {
            var isValid = true;
            $.each($("#tblIMEIItem tbody tr"), function (index, item) {
                //var code = $(this).children("td").eq('5').html();
                //if (code == qrCode) {
                //    isValid = false;
                //}
                var Imei = $(this).children("td").eq('4').html();
                if (Imei.indexOf(imei) != -1) {
                    isValid = false;
                }
            })
            return isValid;
        }

        function bindIMEIData(obj) {
            if (!$.isEmptyObject(obj)) {
                var rowNum = $("#tblIMEIItem tbody tr").length;
                var td1 = '<td class="text-center text-bold">' + (rowNum + 1) + '</td>';
                var td2 = '<td class="text-center">' + $.trim(txtIMEIScaning.val()) + '</td>';
                var td3 = '<td class="text-center">' + $('#ddlItemName :selected').text() + '</td>';
                var td4 = '<td class="text-center"><a href="#" class="btn btn-sm btn-danger btn-flat data-onfly-del"><i class="fas fa-trash"></i></a></td>';
                var td5 = '<td class="hide">' + obj.IMEI + '</td>';
                var td6 = '<td class="hide">' + obj.CodeNo + '</td>';
                var tr = '<tr>' + td1 + td2 + td3 + td4 + td5 + td6 + '</tr>';
                $("#tblIMEIItem tbody").append(tr);
                disable($("#ddlModel"));
                disable($("#ddlItemName"));
                disable($("#ddlPackagingLinePacking"));
            }
        }
        $(document).on("click", "a.data-onfly-del", function (e) {
            e.preventDefault();
            var index = $(this).parent().parents('tbody tr').index();
            removeTableRow("#tblIMEIItem tbody", index);
            fnFixTheTbodyRowSerial("#tblIMEIItem", index);
        })

        function validateCarton() {
            var isValid = true;
            $(".error").addClass("hide");
            if ($("#tblIMEIItem tbody tr").length == 0) {
                $(".empty-data").removeClass('hide');
                isValid = false;
            }
            if (ddlPackagingLinePacking.val() == "") {
                $(".req-pack").removeClass('hide');
                isValid = false;
            }
            if ($.trim(txtGrossWeight.val()) == "") {
                $(".req-grossWeight").removeClass('hide');
                isValid = false;
            }
            if ($.trim(txtNetWeight.val()) == "") {
                $(".req-netWeight").removeClass('hide');
                isValid = false;
            }
            return isValid;
        }



        $("#btnSaveCarton").click(function (e) {
            e.preventDefault();
            if (validateCarton()) {
                var packaging = ddlPackagingLinePacking.val().split("#");
                if (IStockAvailableForCartonSticker(ddlItemName.val(), packaging[1], packaging[0], $("#ddlModel").val(), $("#tblIMEIItem tbody tr").length)) {
                    disable("#btnSaveCarton");
                    disable("#txtIMEIScaning");
                    var info = { LineId: packaging[1], PackagingLineId: packaging[0], GrossWeight: $.trim(txtGrossWeight.val()), NetWeight: $.trim(txtNetWeight.val()), WarehouseId: ddlWarehousePackaging.val(), DescriptionId: $("#ddlModel").val(), TotalQty: $("#tblIMEIItem tbody tr").length, ItemId: TryParseInt(ddlItemName.val(), 0) };
                    var details = []; details.length = 0;
                    $.each($("#tblIMEIItem tbody tr"), function (index, item) {
                        var tds = $(this).children('td');
                        details.push({
                            IMEI: tds.eq('1').html(),
                            QRCode: tds.eq('5').html(),
                            AllIMEI: tds.eq('4').html()
                        });
                    })
                    info.FinishGoodsSendToWarehouseDetails = details;

                    var data = JSON.stringify({ model: info });

                    $.when(postReqWithToken(dataType.applicationJson, dataType.json, type.post, '/Production/SaveFinishGoodsForPacking', data, getToken())).then(function (res, status) {
                        if (status === "success" && res.IsSuccess == true) {
                            toastrSuccessAlert(execuStatus.successSave);
                            printJS({ printable: res.File, type: 'pdf', base64: true });
                            resetFinishGoodsPackagingUI();
                            LoadFinishGoodsTransferList();
                            LoadDataTableStockInfo();
                        }
                        else {
                            toastrErrorAlert(execuStatus.fail)
                        }
                        enable("#btnSaveCarton");
                        enable("#txtIMEIScaning");
                        txtIMEIScaning.val('');
                        txtIMEIScaning.focus();
                    }).fail(function (error) {
                        toastrErrorAlert(execuStatus.fail)
                        console.log(error);
                        enable("#btnSaveCarton");
                        enable("#txtIMEIScaning");
                        txtIMEIScaning.val('');
                        txtIMEIScaning.focus();
                    })
                }
                else {
                    toastrErrorAlert("Stock Is Not Available!");
                }
            }
        })
