@section scripts{
    <script type="text/javascript">

        var hdfInfoId = $("#hdfInfoId");
        var ddlLineNumber = $("#ddlLineNumber");
        var ddlAssembly = $("#ddlAssembly");
        var ddlQCLine = $("#ddlQCLine");
        var ddlModelName = $("#ddlModelName");
        var ddlItems = $("#ddlItems");
        var txtLessOrEq = $("#txtLessOrEq");
        var pageNo = 1;
        var warehouseStock = '';
        var itemTypeStock = '';
        var itemStock = '';

        // Receive Repair Items //

        var ddlFloorReceItem = $('#ddlFloorReceItem');
        var ddlAssemblyReceItem = $("#ddlAssemblyReceItem");
        var ddlQCLineReceItem = $("#ddlQCLineReceItem");
        var ddlRepairLineReceItem = $("#ddlRepairLineReceItem");
        var ddlModelReceItem = $("#ddlModelReceItem");
        var ddlStatusReceItem = $("#ddlStatusReceItem");
        var txtTRQCode = $("#txtTRQCode");
        var dptFromDateReceItem = $("#dptFromDateReceItem");
        var dptToDateReceItem = $("#dptToDateReceItem");

        // QC Pass
        var hdfProductionFloorQP = $("#hdfProductionFloorQP");
        var hdfQCLineQP = $("#hdfQCLineQP");
        var hdfModelQP = $("#hdfModelQP");
        var hdfWarehouseQP = $("#hdfWarehouseQP");
        var hdfItemTypeQP = $("#hdfItemTypeQP");
        var hdfItemQP = $("#hdfItemQP");
        var txtQcPassQty = $("#txtQcPassQty");

        // QC Fail Items //
        var ddlFloorQCFail = $('#ddlFloorQCFail');
        var ddlQCLineQCFail = $("#ddlQCLineQCFail");
        var ddlRepairLineQCFail = $("#ddlRepairLineQCFail");
        var ddlModelNameQCFail = $("#ddlModelNameQCFail");
        var dptFromDateQCFail = $("#dptFromDateQCFail");
        var dptToDateQCFail = $("#dptToDateQCFail");
        var txtTSQCode = $("#txtTSQCode");
        var ddlStateStatusQCFail = $("#ddlStateStatusQCFail");

        $(document).ready(function () {
            loadDataInitializer();
            $(".btnReceive").addClass('hide');
            //Initialize Select2 Elements
            $('.select2').select2();

            //Initialize Select2 Elements
            $('.select2bs4').select2({
                theme: 'bootstrap4'
            });

            // Receive QC Items //
            dptFromDateReceItem.prop('readonly', true);
            dptToDateReceItem.prop('readonly', true);
            dptFromDateReceItem.css("background-color", "#fff");
            dptToDateReceItem.css("background-color", "#fff");
            $('#dptFromDateReceItem').datepicker({
                format: "dd MM yyyy",
                orientation: "bottom auto",
                todayHighlight: true
            });
            $('#dptToDateReceItem').datepicker({
                format: "dd MM yyyy",
                orientation: "bottom auto",
                todayHighlight: true
            });

            // QC Fail //
            dptFromDateQCFail.prop('readonly', true);
            dptToDateQCFail.prop('readonly', true);
            dptFromDateQCFail.css("background-color", "#fff");
            dptToDateQCFail.css("background-color", "#fff");
            $('#dptFromDateQCFail').datepicker({
                format: "dd MM yyyy",
                orientation: "bottom auto",
                todayHighlight: true
            });
            $('#dptToDateQCFail').datepicker({
                format: "dd MM yyyy",
                orientation: "bottom auto",
                todayHighlight: true
            });

            loadDataInitializer();

        })

        function loadDataInitializer() {
            loadReceiveRepairItemDataTable();
            loadQCFailDataTable();
            loadQCItemStock();
            loadQCPassDataTable();
        }

        function getItemsStock() {
            warehouseStock = '';
            itemTypeStock = '';
            itemStock = '';
            if (ddlItems.val() != '') {
                var itemVal = ddlItems.val();
