        function validateQcPassQty() {
            var isValid = true;
            var errorText = "";
            if (TryParseInt(hdfProductionFloorQP.val(), 0) <= 0) {
                errorText += "Floor not found </br>"
                isValid = false;
            }
            if (TryParseInt(hdfQCLineQP.val(), 0) <= 0) {
                errorText += "QC Line not found </br>"
                isValid = false;
            }
            if (TryParseInt(hdfModelQP.val(), 0) <= 0) {
                errorText += "Model not found </br>"
                isValid = false;
            }
            if (TryParseInt(hdfWarehouseQP.val(), 0) <= 0) {
                errorText += "Warehouse not found </br>"
                isValid = false;
            }
            if (TryParseInt(hdfItemTypeQP.val(), 0) <= 0) {
                errorText += "ItemType not found </br>"
                isValid = false;
            }
            if (TryParseInt(hdfItemQP.val(), 0) <= 0) {
                errorText += "Item not found </br>"
                isValid = false;
            }
            if (TryParseInt(txtQcPassQty.val(), 0) <= 0) {
                errorText += "Quantity cannot be 0 or less than 0 </br>"
                isValid = false;
            }
            else {

                if (TryParseInt(hdfProductionFloorQP.val(), 0) > 0 && TryParseInt(hdfQCLineQP.val(), 0) > 0 && TryParseInt(hdfModelQP.val(), 0) > 0 && TryParseInt(hdfItemQP.val(), 0) > 0) {
                    var data = JSON.stringify({ floorId: hdfProductionFloorQP.val(), qcId: hdfQCLineQP.val(), modelId: hdfModelQP.val(), itemId: hdfItemQP.val() });

                    var qty = ajaxValueReturnable(data, '/Common/GetQCItemStockQtyByFloorAndQcAndModelAndItem', getToken())
                    console.log(qty);
                    if (qty != null) {
                        if (TryParseInt(txtQcPassQty.val(), 0) > qty) {
                            errorText += "Qty is exceeded </br>";
                            isValid = false;
                        }
                    }

                    var bundleData = JSON.stringify({ floorId: hdfProductionFloorQP.val(), qcLineId: hdfQCLineQP.val(), modelId: hdfModelQP.val(), itemId: hdfItemQP.val(), qty: TryParseInt(txtQcPassQty.val(), 0), type: preparationType.production });
                    var bundle = ajaxValueReturnable(bundleData, '/Common/GetItemBundleQtyChecking', getToken());
                    console.log("Bundle");
                    console.log(bundle);
                    if (!bundle.isSuccess) {
                        errorText += bundle.text;
                        isValid = false;
                    }
                }

            }
            if (!isValid) {
                bootbox.alert(errorText);
            }

            return isValid;
        }

