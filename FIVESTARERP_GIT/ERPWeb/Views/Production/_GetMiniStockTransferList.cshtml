@model IEnumerable < ERPBO.Production.ViewModels.MiniStockTransferToWarehouseInfoViewModel >
@{ 
    Layout = null;
    int sl = 0;
    string badge = string.Empty;
}
<div class="row">
    <div class="col-md-12">
        <table class="table table-bordered table-sm table-striped table-responsive-lg tblfloor" id="tblfloor">
            <thead class="btn-dark">
                <tr class="text-center">
                    <th>#SL </th>
                    <th class="hide"></th>
                    <th>Code</th>
                    <th>Transfer Status</th>
                    <th>Return Status</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    sl = sl + 1;
                    badge = item.TransferStatus == "Received" ? "badge bg-success" : "badge bg-Warning";
                    <tr>
                        <td class="text-center text-bold">@sl</td>
                        <td class="hide">@item.MSTWInfoId</td>
                        <td>@item.TransferStatus</td>
                        <td>@item.ReturnStatus</td>
                        <td class="text-center">
                            <a href="#" class="btn btn-warning btn-sm data-floor-edit" data-floor-edit="@item.MSTWInfoId"><i class="fa fa-eye"></i> Edit</a>
                        </td>
                    </tr>
                }
                @if (Model.Count() == 0)
                {
                    <tr class="text-center empty-row">
                        <td colspan="9">
                            No data found in the table
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>